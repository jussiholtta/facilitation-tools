{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "rules": {
	    "jest/no-disabled-test": "warn",
	    "jest/no-focused-tests": "error",
	    "jest/no-identical-title": "error",
	    "jest/prefer-to-have-length": "warn",
	    "jest/valid-expect": "error"
    }
}
